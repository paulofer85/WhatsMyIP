<?xml version="1.0"?>
<configuration>

  <appSettings>
    <!-- MAIL CONF DATA -->
    <add key="SMTP" value="mail.server.com.ar"/>
    <add key="SMTPPort" value="587"/>
    <add key="POP3Port" value="110"/>
    <add key="EnableSSL" value="false"/>
    <add key="UseDefaultCredentials" value="false"/>
    <add key="User" value="mail@mail.com.ar"/>
    <add key="Domain" value="domain.com.ar"/>
    <add key="FromAddress" value="mail@mail.com.ar"/>
    <add key="ToAddress" value="mail@gmail.com"/>
    <add key="Password" value="password"/>
    <add key="DeliveryMethod" value="SmtpDeliveryMethod.NetworkToAddress"/>

    <!-- SVC CONF DATA -->
    <!--<add key="TimeInterval" value="60000"/>-->
    <add key="TimeInterval" value="10000"/>
  </appSettings>

  <configProtectedData>
    <providers>
      <add useMachineProtection="true" name="DPAPIProtection"
           type="System.Configuration.DpapiProtectedConfigurationProvider,
                 System.Configuration, Version=2.0.0.0, Culture=neutral, 
                 PublicKeyToken=b03f5f7f11d50a3a" />
    </providers>
  </configProtectedData>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0,Profile=Client"/>
  </startup>
</configuration>
